{
    "rules": {
        "array-callback-return": 2,
        "block-scoped-var": 2,
        "class-methods-use-this": 2,
        "consistent-return": 2,
        "default-case": [2, {"commentPattern": "skip default"}],
        "dot-location": [2, "object"],
        "eqeqeq": [2, "smart"],
        "no-alert": 2,
        "no-caller": 2,
        "no-case-declarations": 2,
        "no-empty-function": 1,
        "no-empty-pattern": 1,
        "no-eval": 2,
        "no-extend-native": 1,
        "no-extra-bind": 1,
        "no-fallthrough": [2, {"commentPattern": "skip break"}],
        "no-floating-decimal": 2,
        "no-global-assign": 2,
        "no-implicit-coercion": 2,
        "no-implicit-globals": 1,
        "no-implied-eval": 2,
        "no-iterator": 2,
        "no-lone-blocks": 1,
        "no-loop-func": 2,
        "no-multi-spaces": 1,
        "no-new": 1,
        "no-new-func": 1,
        "no-new-wrappers": 1,
        "no-octal": 2,
        "no-octal-escape": 2,
        "no-param-reassign": 1,
        "no-proto": 2,
        "no-redeclare": 2,
        "no-return-assign": 2,
        "no-return-await": 2,
        "no-script-url": 2,
        "no-self-assign": 1,
        "no-self-compare": 1,
        "no-throw-literal": 2,
        "no-unmodified-loop-condition": 1,
        "no-useless-escape": 1,
        "no-useless-return": 2,
        "no-warning-comments": 1,
        "prefer-promise-reject-errors": 2,
        "require-await": 1,
        "vars-on-top": 1,
        "wrap-iife": [2, "any"]
    }
}
