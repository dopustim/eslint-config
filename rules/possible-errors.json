{
    "rules": {
        "no-console": 1,
        "no-control-regex": 1,
        "no-dupe-args": 2,
        "no-dupe-keys": 2,
        "no-duplicate-case": 2,
        "no-empty": 1,
        "no-empty-character-class": 2,
        "no-ex-assign": 2,
        "no-extra-parens": 1,
        "no-extra-semi": 1,
        "no-func-assign": 2,
        "no-inner-declarations": 1,
        "no-invalid-regexp": 2,
        "no-irregular-whitespace": 2,
        "no-obj-calls": 2,
        "no-regex-spaces": 2,
        "no-unexpected-multiline": 1,
        "no-unreachable": 2,
        "no-unsafe-finally": 2
    }
}
